{
  "name": "mtg-ocelot-pride-calculator",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "test": "bun run lint && bun test",
    "test:watch": "bun test --watch",
    "watch": "bun build src/app/index.tsx --outdir ./out --watch",
    "serve": "bunx --hot serve out",
    "lint": "bun run typescript:check && bun run eslint:check && bun run prettier:check",
    "typescript:check": "tsc --noEmit",
    "eslint:check": "eslint . --max-warnings 0",
    "eslint:format": "eslint . --fix",
    "prettier:check": "prettier --check --config './.prettierrc.js' --ignore-path './.prettierignore' .",
    "prettier:format": "prettier --write --config './.prettierrc.js' --ignore-path './.prettierignore' ."
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.1.6",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "type-samurai": "^1.1.1"
  }
}
